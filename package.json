{
  "name": "url-shortener-dio",
  "version": "1.0.0",
  "description": "URL Shortener for DIO Project",
  "main": "index.js",
  "engines": {
    "node": "16.x",
    "npm": "8.x"
  },
  "scripts": {
    "eslint": "eslint src/**/*.ts",
    "eslint:fix": "eslint --fix src/**/*.ts",
    "prettier": "prettier src/**/*.ts",
    "prettier:fix": "prettier --write src/**/*.ts",
    "build:watch": "tsc -w",
    "build": "tsc -p .",
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules --no-notify src/index.ts",
    "start": "node ./dist/index.js",
    "test": "jest"
  },
  "author": "alexiadorneles",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.1",
    "express-rate-limit": "^6.2.1",
    "helmet": "^5.0.2",
    "mongoose": "^6.2.2",
    "nanoid": "^3.3.1",
    "swagger-ui-express": "^4.3.0"
  },
  "devDependencies": {
    "@typegoose/typegoose": "^9.6.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.8",
    "@types/jest": "^27.4.0",
    "@types/supertest": "^2.0.11",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "jest": "^27.5.1",
    "prettier": "2.5.1",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.0.2"
  }
}
